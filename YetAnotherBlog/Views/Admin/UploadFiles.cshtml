@model YetAnotherBlog.Models.UploadViewModel

@{
    ViewData["Title"] = "UploadFiles";
}

@await Html.PartialAsync("_sidebar")

<div class="col-md-8">
    <h2>Upload Files</h2>

    <form asp-action="UploadFiles">
        <input type="file" asp-for="File" />
        <button type="submit">Upload</button>
    </form>
</div>

